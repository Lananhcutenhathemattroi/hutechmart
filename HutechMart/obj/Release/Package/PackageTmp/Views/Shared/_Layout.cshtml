<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Hutech Mart</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bbootstrap 4 -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/Assets/Admin_Layout/plugins/summernote/summernote-bs4.css">
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
    <link href="~/Assets/Admin_Layout/js/admin/jquery-ui.css" rel="stylesheet" />
    <link href="~/Assets/sweetalert2-10.10.1/package/dist/sweetalert2.min.css" rel="stylesheet" />

    <link href="~/Assets/kendoui.for.jquery.2020.3.915.trial/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="~/Assets/kendoui.for.jquery.2020.3.915.trial/styles/kendo.default.min.css" rel="stylesheet" />
    <link href="~/Assets/kendoui.for.jquery.2020.3.915.trial/styles/kendo.default.mobile.min.css" rel="stylesheet" />

    @*<script src="~/Assets/kendoui.for.jquery.2020.3.915.trial/js/jquery.min.js"></script>*@

    <script src="~/Assets/sweetalert2-10.10.1/package/dist/sweetalert2.min.js"></script>
</head>

<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-primary navbar-dark">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/Home/Index" class="brand-link" style="margin-left:15px">
                <span class="brand-text font-weight-light"><b>HUTECH MART</b></span>
            </a>
            @Html.Partial("_SidebarParial")
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="content-header">
                <div class="container-fluid">
                    <div class="row mb-2">
                        <div class="col-sm-6">
                            <h3 class="m-0 text-dark">@ViewBag.Title</h3>
                        </div>
                    </div>
                </div>
            </div>
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2020 <a href="https://www.facebook.com/vqhuyz/">Vũ Quốc Huy</a>.</strong>
            <div class="float-right d-none d-sm-inline-block">
                <b>Hutech</b> Mart
            </div>
        </footer>
    </div>
    <script src="~/Assets/Admin_Layout/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/Assets/Admin_Layout/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <!-- Bootstrap 4 -->
    <script src="~/Assets/Admin_Layout/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/Assets/Admin_Layout/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Assets/Admin_Layout/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/Assets/Admin_Layout/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/Assets/Admin_Layout/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/Assets/Admin_Layout/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/Assets/Admin_Layout/plugins/moment/moment.min.js"></script>
    <script src="~/Assets/Admin_Layout/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/Assets/Admin_Layout/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/Assets/Admin_Layout/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Assets/Admin_Layout/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Assets/Admin_Layout/dist/js/adminlte.js"></script>
    <script src="~/Assets/Admin_Layout/js/admin/jquery-1.12.4.js"></script>
    <script src="~/Assets/Admin_Layout/js/admin/jquery-ui.js"></script>
    <script src="~/Assets/sweetalert2-10.10.1/package/dist/sweetalert2.all.min.js"></script>

    <script src="~/Assets/kendoui.for.jquery.2020.3.915.trial/js/kendo.all.min.js"></script>

    @RenderSection("Scripts", required: false)

</body>
</html>


